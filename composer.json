{
    "name": "d3vy/slim-skeleton",
    "description": "Simple skeleton for Slim Framework.",
    "keywords": ["slim", "skeleton", "php-framework"],
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/d3vy/Slim-skeleton",
    "authors": [
        {
            "name": "Szabolcs SurÃ¡nyi",
            "email": "szabolcs.suranyi@shadesoft.hu",
            "homepage": "http://shadesoft.hu"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.5",
        "monolog/monolog": "^1.0",
        "slim/php-view": "^2.0",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.0",
        "symfony/asset": "^3.0|^4.0",
        "symfony/yaml": "^3.0|^4.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.0|^4.0",
        "zeuxisoo/slim-whoops": "^0.6"
    },
    "scripts": {
        "post-install-cmd": [
            "App\\ScriptHandler::createParametersFile"
        ],
        "post-update-cmd": [
            "App\\ScriptHandler::createParametersFile"
        ],
        "start": "php -S localhost:8080 -t public"
    },
    "config": {
        "platform": {},
        "process-timeout": 0,
        "sort-packages": true
    }
}